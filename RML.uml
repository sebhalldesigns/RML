@startuml
hide empty description

[*] --> ROOT

ROOT --> READY : (LEVEL_=_0)

READY --> BEGIN_TAG : <
READY --> SYNTAX_ERROR: Else

READY --> CONTENTS : Text_AND_LEVEL_>_0

CONTENTS --> BEGIN_TAG : <
CONTENTS --> CONTENTS : Else

BEGIN_TAG --> CLASS : Text
BEGIN_TAG --> CLOSING_TAG : /
BEGIN_TAG --> SYNTAX_ERROR : Else

CLASS --> CLASS : Text
CLASS --> TAG_READY : SPACE
CLASS --> SINGLE_TAG_CLOSING : /
CLASS --> PARTIAL_COMPLETE: >
CLASS --> SYNTAX_ERROR : Else

TAG_READY --> KEY : Text
TAG_READY --> TAG_READY : SPACE
TAG_READY --> SINGLE_TAG_CLOSING : /
TAG_READY --> PARTIAL_COMPLETE : >
TAG_READY --> SYNTAX_ERROR : Else

KEY --> EQUALS : =
KEY --> KEY : SPACE
KEY --> SYNTAX_ERROR : Else

EQUALS --> VALUE : "
EQUALS --> EQUALS : SPACE
EQUALS --> SYNTAX_ERROR : Else

VALUE --> VALUE : Text
VALUE --> VALUE : SPACE
VALUE --> TAG_READY : "
VALUE --> SYNTAX_ERROR : Else

CLOSING_TAG --> CLOSING_CLASS : Text
CLOSING_TAG --> SYNTAX_ERROR : Else

CLOSING_CLASS --> CLOSING_CLASS : Text
CLOSING_CLASS --> COMPLETE : >
CLOSING_CLASS --> SYNTAX_ERROR : Else

SINGLE_TAG_CLOSING --> COMPLETE : >
SINGLE_TAG_CLOSING --> SYNTAX_ERROR : Else

PARTIAL_COMPLETE --> READY : (LEVEL_INCREMENT)

COMPLETE --> [*] : LEVEL_=_0
COMPLETE --> READY : LEVEL_>_0->(LEVEL_DECREMENT)




@enduml
